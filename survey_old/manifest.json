{
    "name": "Agile Survey",
    "description": "This is a survey for a college resource.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "src/background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "src/interface/popup.html"
    }
}